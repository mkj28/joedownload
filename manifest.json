    {
      "manifest_version": 3,
      "name": "Joe MP4 Downloader",
      "version": "1.0",
      "description": "Downloads the embedded MP4 video from the current page.",
      "permissions": [
        "activeTab",
        "scripting",
        "downloads"
      ],
      "action": {
        "default_popup": "",
        "default_icon": {
          "16": "icons/icon16.png",
          "48": "icons/icon48.png",
          "128": "icons/icon128.png"
        },
        "default_title": "Download MP4"
      },
      "background": {
        "service_worker": "background.js"
      },
      "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }

    }
